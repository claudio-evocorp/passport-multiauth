{
    "name": "claudiorodrigo/passport-multiauth",
    "description": "Add support to multi-auth on Laravel Passport",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "passport", "multi-auth"],
    "authors": [
        {
            "name": "Claudio Rodrigo",
            "email": "claudio@evocorp.com.br"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "laravel/passport": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CRodrigo\\PassportMultiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "CRodrigo\\PassportMultiauth\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
